"toggleTransaction","generated":{"line":98,"column":22},"source":"components/Block.js","original":{"line":47,"column":34}},{"generated":{"line":99,"column":0},"source":"components/Block.js","original":{"line":44,"column":16}},{"generated":{"line":99,"column":21},"source":"components/Block.js","original":{"line":41,"column":12}},{"generated":{"line":99,"column":22},"source":"components/Block.js","original":{"line":40,"column":8}},{"generated":{"line":100,"column":0},"source":"components/Block.js","original":{"line":53,"column":5}},{"name":"Component","generated":{"line":104,"column":2},"source":"components/Block.js","original":{"line":5,"column":20}},{"name":"Block","generated":{"line":106,"column":15},"source":"components/Block.js","original":{"line":70,"column":15}}],"sources":{"components/Block.js":"import React, { Component } from 'react';\nimport { Button } from 'react-bootstrap';\nimport Transaction from './Transaction';\n\nclass Block extends Component {\n    state = { displayTransaction: false };\n\n    toggleTransaction = () => {\n        this.setState({displayTransaction: !this.state.displayTransaction});\n    };\n\n    get displayTransaction() {\n        const { data } = this.props.block;\n        const stringData = JSON.stringify(data);\n        const dataDisplay = stringData.length > 35 ? `${stringData.substring(0, 35)}...` : stringData;\n\n        if (this.state.displayTransaction) {\n            return(\n                <div>\n                    {\n                        data.map(transaction => (\n                            <div key={transaction.id}>\n                                <hr />\n                                <Transaction transaction={transaction} />\n                            </div>\n                        ))\n                    }\n                    <br />\n                    <Button\n                        bsStyle=\"danger\"\n                        bsSize=\"small\"\n                        onClick={this.toggleTransaction}\n                    >\n                        Show Less\n                    </Button>\n                </div>\n            );\n        }\n\n        return  (\n            <div>\n                <div>Data: {dataDisplay}</div>\n                <br />\n                <Button\n                    bsStyle=\"danger\"\n                    bsSize=\"small\"\n                    onClick={this.toggleTransaction}\n                >\n                    Show More\n                </Button>\n            </div>\n        );\n    }\n\n\n    render() {\n        const { timeStamp, hash } = this.props.block;\n        const hashDisplay = `${hash.substring(0, 15)}...`;\n        return (\n            <div className='Block'>\n                <div>Hash: {hashDisplay}</div>\n                <div>Timestamp: {new Date(timeStamp).toLocaleString()}</div>\n                {this.displayTransaction}\n            </div>\n        );\n    }\n\n}\n\nexport default Block;"},"lineCount":null}},"hash":"a77f56b26d08d07bc85c4df1b88296a4","cacheData":{"env":{}}}